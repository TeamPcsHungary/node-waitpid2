{
  "name": "waitpid2",
  "version": "2.0.6",
  "description": "Complete native binding to the waitpid() syscall",
  "main": "lib/waitpid.js",
  "scripts": {
    "install": "node-gyp rebuild"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshiggins/node-waitpid2.git"
  },
  "keywords": [
    "waitpid",
    "child process"
  ],
  "author": "joshiggins",
  "license": "MIT",
  "gypfile": true,
  "dependencies": {
    "node-pre-gyp": "^0.6.33"
  },
  "devDependencies": {
    "node-pre-gyp-github": "^1.3.1"
  },
  "bundledDependencies":["node-pre-gyp"],
  "scripts": {
    "prepublish": "npm ls",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "binary": {
    "module_name": "waitpid",
    "module_path": "./lib/binding/",
    "host": "https://github.com/takacsv/node-waitpid2/releases/download/",
    "remote_path": "{version}"
  }
}
